<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>RegistMember</servlet-name>
		<servlet-class>kr.ac.shinhan.csp.RegistMemberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegistMember</servlet-name>
		<url-pattern>/registmember</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet>
		<servlet-name>MemberInfo</servlet-name>
		<servlet-class>kr.ac.shinhan.csp.MemberInfoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MemberInfo</servlet-name>
		<url-pattern>/memberinfo</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DeleteMember</servlet-name>
		<servlet-class>kr.ac.shinhan.csp.DeleteMemberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteMember</servlet-name>
		<url-pattern>/deletemember</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>UpdateMember</servlet-name>
		<servlet-class>kr.ac.shinhan.csp.UpdateMemberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateMember</servlet-name>
		<url-pattern>/updatemember</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
